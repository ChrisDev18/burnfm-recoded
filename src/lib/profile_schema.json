{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "default": [],
  "title": "Root",
  "description": "List of profiles",
  "items": {
    "type": "object",
    "title": "A Profile Object",
    "required": [
      "name",
      "role",
      "course",
      "picture",
      "fun_fact",
      "favourite_song",
      "description"
    ],
    "properties": {
      "role": {
        "type": "string",
        "title": "The person's role on committee"
      },
      "name": {
        "type": "string",
        "title": "The person's name"
      },
      "course": {
        "type": "string",
        "title": "The course the person is taking"
      },
      "picture": {
        "type": "string",
        "title": "The link to a picture of the person",
        "examples": [
          "/profile_img/path_to_img"
        ]
      },
      "fun_fact": {
        "type": "string",
        "title": "The person's fun fact"
      },
      "favourite_song": {
        "type": "string",
        "title": "A Spotify link to the person's favourite song"
      },
      "description": {
        "type": "string",
        "title": "A description of the person and their role"
      }
    },
    "examples": [{
      "name": "",
      "role": "",
      "course": "",
      "picture": "/profile_img/img.png",
      "fun_fact": "",
      "favourite_song": "",
      "description": ""
    },
      {
        "name": "",
        "role": "",
        "course": "",
        "picture": "/profile_img/img.png",
        "fun_fact": "",
        "favourite_song": "",
        "description": ""
      }]
  },
